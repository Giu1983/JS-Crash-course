}
            

              addCity(city) {
             this.cities.push(city)
}

//Create an Offer class




var Offer = class {

  const(date, number, price) {

    this.date = date

    this.number = number

    this.price = price

  }




  sayUserName() {

    console.log('My name is', this.name)

  }




  sayShopPlace() {

    console.log('The shop is', this.place)

  }




  Set Price (offer) {

    console.log('It cost EUR', this.price)

    offer.price = this.price

    

  }

}




